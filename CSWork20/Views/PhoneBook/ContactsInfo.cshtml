@using CSWork20.Enities;
<head>
    <title>Задание SkillBox C# 19</title>
    <meta charset="utf-8">
    <link type="text/css" rel="stylesheet" href="~/css/style.css">
</head>

<div class="table">
    <div class="row">
        <div class="cell">ID</div>
        <div class="cell">@ViewBag.contact.Id</div>
    </div>
    <div class="row">
        <div class="cell">Имя</div>
        <div class="cell">@ViewBag.contact.FirstName</div>
    </div>
    <div class="row">
        <div class="cell">Фамилия</div>
        <div class="cell">@ViewBag.contact.LastName</div>
    </div>
    <div class="row">
        <div class="cell">Отчество</div>
        <div class="cell">@ViewBag.contact.ThirdName</div>
    </div>
    <div class="row">
        <div class="cell">Телефон</div>
        <div class="cell">@ViewBag.contact.Phone</div>
    </div>
    <div class="row">
        <div class="cell">Аддресс</div>
        <div class="cell">@ViewBag.contact.Address</div>
    </div>
    <div class="row">
        <div class="cell">Дескрипшен</div>
        <div class="cell">@ViewBag.contact.Desc</div>
    </div>
</div>

<a href="/PhoneBook/ContactsList">Вернуться с списку</a>